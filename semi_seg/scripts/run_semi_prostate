#!/bin/bash
#!/bin/bash
#base_folder=/lustre04/scratch/jizong/Contrast-You/semi_seg/runs/0401_acdc/githash_4519149/acdc
base_folder=/home/jizong/Workspace/Contrast-You/semi_seg/runs/0401/githash_2756201
save_dir=prostate_semi

cmd_cmd="--show_cmd"
# random_seed 10

source ./run_semi $save_dir/semi_supervised \
  prostate \
  $cmd_cmd \
  10

# running all things without depending on the checkpoint
run_null_checkpoint() {
  run_baseline no_checkpoint_path null
  run_mt no_checkpoint_path null
}

run_semi_on_checkpoint() {
  run_baseline $1 yes $2
  run_mt $1 yes $2
  run_infonce $1 yes $2
  run_infonce $1 null $2
  run_mt_infonce $1 yes $2
  run_mt_infonce $1 null $2
}

#run_null_checkpoint
run_semi_on_checkpoint ${base_folder}/prostate/random_seed_10/sample_num_5/global_Conv5_1.0/contrast_on_partition/self-paced/method_hard/loss_params*5.0_16.0 \
  contrast_on_parition/self
run_semi_on_checkpoint ${base_folder}/prostate/random_seed_10/sample_num_5/global_Conv5_1.0/contrast_on_partition/infonce \
  contrast_on_parition/infonce

run_semi_on_checkpoint ${base_folder}/prostate/random_seed_10/sample_num_5/global_Conv5_1.0/contrast_on_patient/self-paced/method_hard/loss_params*4.0_16.0 \
  contrast_on_patient/self

run_semi_on_checkpoint ${base_folder}/prostate/random_seed_10/sample_num_5/global_Conv5_1.0/contrast_on_patient/infonce \
  contrast_on_patient/infonce
