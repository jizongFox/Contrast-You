#!/bin/bash

export CUBLAS_WORKSPACE_CONFIG=:4096:8


SAMPLE_PATH=/home/jizong/Workspace/Contrast-You/runs/beluga/1224_combine/acdc/hash_955663565fd/acdc/pretrain/sample

