  trainer_checkpoint=${SAMPLE_PATH}/rr_alpha_1/pretrain/last.pth CrossCorrelationParameters.hooks.rr.alpha=0 \
  --path ${SAMPLE_PATH}/rr_alpha_1/pretrain/config.yaml

